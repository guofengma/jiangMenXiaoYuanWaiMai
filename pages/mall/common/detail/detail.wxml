<!--订单详情  -->
<template name="detail">

<view class="nums btap" wx:if="{{wayName}}">
<text>配送信息</text>
<text>{{wayName}}</text>
</view>
<view class="nums" wx:if="{{orderFreight}}">
<text>运费</text>
<text>¥{{orderFreight}}</text>
</view>
<view class="nums" wx:if="{{payTypeTxt}}">
<text>支付方式</text>
<text>{{payTypeTxt}}</text>
</view>

 <view class="nums" wx:if="{{ticketTxt}}">
<text>优惠劵</text>
<text>{{ticketTxt}}</text>
</view> 

<view class="nums">
<text>订单号</text>
<text>{{orderSn}}</text>
</view>

<view class="nums">
<text>状态</text>
<text class="staus_txt">{{orderStateTxt}}</text>
</view>
